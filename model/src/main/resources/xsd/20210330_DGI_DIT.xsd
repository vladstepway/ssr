            <!--уном рсселяемого дома-->
            <!--кадастровый номер рсселяемого дома-->
            <!--№ квартирырсселяемого дома-->
            <!--№ комнат рсселяемого дома-->
            <!--текст ошибки-->
        </xs:sequence>
    </xs:complexType>
