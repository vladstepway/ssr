        <xs:complexType>
            <xs:all>
                <xs:element name="p_unom" type="xs:int" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>УНОМ</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="p_cadastr" type="xs:string" minOccurs="0" maxOccurs="1">
                    <xs:annotation>
