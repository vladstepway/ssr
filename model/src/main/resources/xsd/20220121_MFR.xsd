<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified"
           xmlns="http://www.ugd.croc.ru/ssr/model/integration/mfr"
           targetNamespace="http://www.ugd.croc.ru/ssr/model/integration/mfr">
    <!--1. Сведения о начале отселения-->
    <xs:element name="dgpToMfrResettlementInfo">
        <xs:annotation>
            <xs:documentation>Сведения о начале отселения</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="resettlementInfo">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="resettlementType" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation>Тип отселения
                                        1 - Полное
                                        2 - Частичное
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="fromUnom" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation>УНОМ отселяемого дома</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="fromCadNum" type="xs:string" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>Кадастровый номер отселяемого дома</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="toUnom" type="xs:string" maxOccurs="unbounded">
                                <xs:annotation>
                                    <xs:documentation>УНОМ заселяемого дома</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="startDate" type="xs:date">
                                <xs:annotation>
                                    <xs:documentation>Дата начала отселения</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="fromFlats" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="flatNumber" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation>Номер квартиры отселяемого дома</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="roomNumbers" type="xs:string" minOccurs="0" maxOccurs="unbounded">
                                <xs:annotation>
                                    <xs:documentation>Номера комнат отселяемого дома (Обязательно в случае с
                                        коммунальной
                                        квартирой)
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="roomAmount" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation>Количество комнат отселяемого помещения</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="floor" type="xs:string" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>Этаж отселяемого помещения</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="totalSquare" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation>Общая площадь отселяемого помещения</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="livingSquare" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation>Жилая площадь отселяемого помещения</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="resettlementDirection" type="xs:string" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>Направление переселения
                                        1 - Компенсация
                                        2 - Вне района
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="affairId" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation>Уникальный код помещения/семьи
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="statusLiving" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation>Статус проживания</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="hasDisabledPerson" type="xs:boolean">
                                <xs:annotation>
                                    <xs:documentation>Признак наличия в отселяемом помещении инвалида-колясочника
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="hasWaiterPerson" type="xs:boolean">
                                <xs:annotation>
                                    <xs:documentation>Признак наличия в отселяемом помещении очередника
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="cip">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="unomCip" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation>UNOM ЦИП</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="addressCip" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation>Адрес ЦИП</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="phoneCip" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation>Телефон ЦИП</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="workCip" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation>Режим работы ЦИП</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="dateCip" type="xs:date">
                                <xs:annotation>
                                    <xs:documentation>Дата открытия ЦИП</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- 2. Сведения о РД при равнозначном переселении-->
    <xs:element name="dgpToMfrAdministrativeDocumentsInfo">
        <xs:annotation>
            <xs:documentation>Сведения о РД при равнозначном переселении</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="affairId" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>Уникальный код помещения/семьи (Из потока №1 "Сведения о начале отселения")
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="newUnom" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>UNOM заселяемого дома</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="newFlatNumber" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>Номер квартиры заселяемого дома</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="newRoomAmount" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>Количество комнат в заселяемой квартире</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="newLivingSquare" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Жилая площадь в заселяемой квартире</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="newTotalSquare" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Общая площадь в заселяемой квартире</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="unom" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>UNOM отселяемого дома</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="fromFlat">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="flatNumber" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation>Номер отселяемой квартиры</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="roomNumbers" type="xs:string" minOccurs="0" maxOccurs="unbounded">
                                <xs:annotation>
                                    <xs:documentation>Номера отселяемых комнат (Обязательно в случае с коммунальной
                                        квартирой)
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="flowReceiveDate" type="xs:date">
                    <xs:annotation>
                        <xs:documentation>Дата поступления потока в ИАС УГД</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- 3. Сведения о проекте договора при равнозначном переселении-->
    <xs:element name="dgpToMfrContractProjectInfo">
        <xs:annotation>
            <xs:documentation>Сведения о проекте договора при равнозначном переселении</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="affairId" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>Уникальный код помещения/семьи (Из потока №1 "Сведения о начале отселения")
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="orderId" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>Уникальный номер договора</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="newUnom" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>UNOM заселяемого дома</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="newFlatNumber" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>Номер квартиры заселяемого дома</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="unom" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>UNOM отселяемого дома</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="fromFlat">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="flatNumber" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation>Номер отселяемой квартиры</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="roomNumbers" type="xs:string" minOccurs="0" maxOccurs="unbounded">
                                <xs:annotation>
                                    <xs:documentation>Номера отселяемых комнат (Обязательно в случае с коммунальной
                                        квартирой)
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="contractDate" type="xs:date">
                    <xs:annotation>
                        <xs:documentation>Дата проекта договора</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="contractStatus" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>Статус
                            1 - Аннулирован
                            2 - Подготовлен
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- 4. Сведения о договоре при равнозначном переселении-->
    <xs:element name="dgpToMfrContractInfo">
        <xs:annotation>
            <xs:documentation>Сведения о договоре при равнозначном переселении</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="affairId" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>Уникальный код помещения/семьи (Из потока №1 "Сведения о начале отселения")
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="orderId" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>Уникальный номер договора</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="newUnom" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>UNOM заселяемого дома</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="newFlatNumber" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>Номер квартиры заселяемого дома</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="contractDate" type="xs:date">
                    <xs:annotation>
                        <xs:documentation>Дата договора</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="actDate" type="xs:date">
                    <xs:annotation>
                        <xs:documentation>Дата акта приёма-передачи</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="unom" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>UNOM отселяемого дома</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="fromFlat">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="flatNumber" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation>Номер отселяемой квартиры</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="roomNumbers" type="xs:string" minOccurs="0" maxOccurs="unbounded">
                                <xs:annotation>
                                    <xs:documentation>Номера отселяемых комнат (Обязательно в случае с коммунальной
                                        квартирой)
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="contractStatus" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>Статус
                            1 - Аннулирован
                            2 - Подписан
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- 5. Сведения о предложениях компенсации и вне района-->
    <xs:element name="mfrToDgpCompensationOfferInfo">
        <xs:annotation>
            <xs:documentation>Сведения о предложениях компенсации и вне района</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="affairId" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>Уникальный код помещения/семьи</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="sellId" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>Уникальный номер реализации</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="chedFileId" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>PDF-файл с предложением (обезличенный) - ссылка на ЦХЭД</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="newFlatNumber" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>Номер заселяемой квартиры</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="newUnom" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>UNOM заселяемого дома</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="offerLetterDate" type="xs:date">
                    <xs:annotation>
                        <xs:documentation>Дата письма с предложением</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="offerType" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>Тип предложения
                            1 - Компенсация
                            2 - Вне района
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- 6. Сведения о статусе предложения по компенсации-->
    <xs:element name="mfrToDgpCompensationOfferStatus">
        <xs:annotation>
            <xs:documentation>Сведения о статусе предложения по компенсации</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="sellId" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>Уникальный номер реализации (Из потока №5 "Сведения о предложениях
                            компенсации")
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="affairId" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>Уникальный код помещения/семьи</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="date" type="xs:date">
                    <xs:annotation>
                        <xs:documentation>Дата</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="offerStatus" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Статус предложения
                            1 - Согласие
                            2 - Отказ
                            3 - Аннулировано
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="unoms" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="unom" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation>УНОМ предложенного дома</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="flats" maxOccurs="unbounded">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="flatNumber" type="xs:string">
                                            <xs:annotation>
                                                <xs:documentation>Номер предложенной квартиры</xs:documentation>
                                            </xs:annotation>
                                        </xs:element>
                                        <xs:element name="cadNum" type="xs:string" minOccurs="0">
                                            <xs:annotation>
                                                <xs:documentation>Кадастровый номер квартиры</xs:documentation>
                                            </xs:annotation>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- 7. Сведения о заявлении по докупке-->
    <xs:element name="mfrToDgpBuyInClaimInfo">
        <xs:annotation>
            <xs:documentation>Сведения о заявлении по докупке</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="affairId" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>Уникальный код помещения/семьи (Из потока №1 "Сведения о начале отселения")
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="sellId" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>Уникальный номер реализации</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="claimDate" type="xs:date">
                    <xs:annotation>
                        <xs:documentation>Дата заявления</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="buyInType" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>Вид докупки
                            1 - Докупка
                            2 - Докупка с компенсацией
                            3 - Покупка в течение 2-х лет
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="unoms" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="unom" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation>UNOM дома докупки/переселения</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="flats" maxOccurs="unbounded">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="flatNumber" type="xs:string">
                                            <xs:annotation>
                                                <xs:documentation>Номер квартиры</xs:documentation>
                                            </xs:annotation>
                                        </xs:element>
                                        <xs:element name="cadNum" type="xs:string" minOccurs="0">
                                            <xs:annotation>
                                                <xs:documentation>Кадастровый номер квартиры</xs:documentation>
                                            </xs:annotation>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- 8. Сведения о жителях-->
    <xs:element name="dgpToMfrPersonInfo">
        <xs:annotation>
            <xs:documentation>Сведения о жителях</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
